# Configuration des API
# Copiez ce fichier en .env et remplacez les valeurs par vos clés API

# OpenAI API Key (pour la génération de descriptions)
REACT_APP_OPENAI_API_KEY=votre_clé_openai_ici

# Configuration optionnelle pour d'autres services d'IA
# REACT_APP_STABLE_DIFFUSION_API_KEY=votre_clé_stable_diffusion_ici

# Configuration des images
REACT_APP_MAX_IMAGE_SIZE=5242880  # 5MB en octets
REACT_APP_DEFAULT_IMAGE_WIDTH=800
REACT_APP_DEFAULT_IMAGE_HEIGHT=600